{{>layout/header}}

<div class="col-md-12">
    <h3 class="header">Join Page</h3>
    <div class="col-md-4">
        <form>
            <div class="form-group">
                <label for="id">ID:</label>
                <input id="id" type="text" class="form-control" placeholder="username"/>
            </div>
            <div class="form-group">
                <label for="passwd">Password:</label>
                <input id="passwd" type="password" class="form-control" placeholder="password"/>
            </div>
        </form>
        <a href="/" role="button" class="btn btn-secondary">취소</a>
        <button type="button" class="btn btn-primary" id="join-btn">Join</button>
    </div>
</div>

{{>layout/footer}}